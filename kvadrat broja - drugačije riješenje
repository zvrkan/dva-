<?php

$a = array();

for($i=1;$i<=5;$i++)
{
    $b = readline("Upisite $i. broj: ");

    $a[$i]=$b;
}

$c = readline("Upisite kvadratni broj");
$d = 0;
foreach($a as $key => $val)
{
    $g = $c;
    $f = $c;
    
    if($val == $g*$f)
    {
        $d += 1;
    }
    else
    {}
}
if($d > 0)
{
    echo "Kvadrat broja $c se nalazi u polju.";
}
else
{
    echo "Kvadrat broja $c se ne nalazi u polju;";
}

?>
